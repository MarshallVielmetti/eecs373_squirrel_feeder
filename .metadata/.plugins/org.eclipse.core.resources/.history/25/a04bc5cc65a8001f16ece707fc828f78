/* USER CODE BEGIN 4 */

/*
 * mounts the sd card -- if its not found, it blocks forever
 */
void sdcard_init(void) {
	fres = f_mount(&FatFs, "", 1); //1=mount now
	if (fres != FR_OK) {
		while(1);
	}
}

/*
 * Unmounts the sd card
 */
void sdcard_cleanup(void) {
	f_mount(NULL, "", 0);
}

/* USER CODE END 4 */

